<?xml version="1.0"?>
<Container version="2">
  <Name>personal-automation-api</Name>
  <Repository>dylansheffer/personal-automation-api</Repository>
  <Registry>https://hub.docker.com/r/dylansheffer/personal-automation-api/</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>https://github.com/dylansheffer/personal-automation-api</Support>
  <Project>https://github.com/dylansheffer/personal-automation-api</Project>
  <Overview>Personal Automation API for YouTube notes and summaries</Overview>
  <Category>Productivity: Tools:</Category>
  <WebUI>http://[IP]:[PORT:8000]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/dylansheffer/personal-automation-api/main/my-template.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/dylansheffer/personal-automation-api/main/icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1685620800</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="API_KEY" Target="API_KEY" Default="" Mode="" Description="Your API key" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="OPENAI_API_KEY" Target="OPENAI_API_KEY" Default="" Mode="" Description="Your OpenAI API key" Type="Variable" Display="always" Required="true" Mask="true"/>
  <Config Name="WebUI" Target="8000" Default="8000" Mode="tcp" Description="Web UI Port" Type="Port" Display="always" Required="true" Mask="false"/>
</Container>